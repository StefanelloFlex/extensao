{
    "version": "3.1.0",
    "name": "Redmine Flexmobile",
    "description": "Ajustes na página e notificações",
    "icons": {
        "128": "128.png"
    },
    "permissions": [
        "storage",
        "notifications"
    ],
    "background": {
        "service_worker": "watch.js"
    },
    "options_ui": {
        "page": "settings.html",
        "open_in_tab": false
    },
    "content_scripts": [
        {
            "matches": [
                "*://sac.flexmobile.com.br/redmine/*",
                "*://192.168.1.5/redmine/*"
            ],
            "js": [
                "hide.js",
                "page.js"
            ],
            "css": [
                "width.css"
            ]
        }
    ],
    "manifest_version": 3
}
